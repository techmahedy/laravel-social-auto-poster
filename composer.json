{
    "name": "laravelia/autoposter",
    "description": "A Laravel package for auto posting social site",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Laravelia\\Autoposter\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Mahedi Hasan"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "tumblr/tumblr": "dev-master"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravelia\\Autoposter\\AutoPosterServiceProvider"
            ]
        }
    }
}
